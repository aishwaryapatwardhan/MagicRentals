<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="150dp"
        android:id="@+id/iconImage"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:id="@+id/heartsImage"/>

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_margin="10dp">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Address:"
        android:layout_margin="10dp"
        android:layout_gravity="left"
        style="@style/Text1"/>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Street:"
        android:layout_margin="10dp"
        android:layout_gravity="left"
        style="@style/Text1"/>


    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="City:"
        android:layout_margin="10dp"
        android:layout_gravity="left"
        style="@style/Text1"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="State:"
        android:layout_margin="10dp"
        android:layout_gravity="left"
        style="@style/Text1"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ZipCode:"
        android:layout_margin="10dp"
        android:layout_gravity="left"
        style="@style/Text1"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Property Type:"
        android:layout_margin="10dp"
        android:layout_gravity="left"
        style="@style/Text1"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="# of Rooms:"
        android:layout_margin="10dp"
        android:layout_gravity="left"
        style="@style/Text1"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="# of baths:"
        android:layout_margin="10dp"
        android:layout_gravity="left"
        style="@style/Text1"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sq Feet:"
        android:layout_margin="10dp"
        android:layout_gravity="left"
        style="@style/Text1"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Monthly rent:"
        android:layout_margin="10dp"
        android:layout_gravity="left"
        style="@style/Text1"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Description:"
        android:layout_margin="10dp"
        android:layout_gravity="left"
        style="@style/Text1"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Deposit:"
        android:layout_margin="10dp"
        android:layout_gravity="left"
        style="@style/Text1"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lease Type:"
        android:layout_margin="10dp"
        android:layout_gravity="left"
        style="@style/Text1"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Contact number:"
        android:id="@+id/textView14"
        android:layout_margin="10dp"
        android:layout_gravity="left"
        style="@style/Text1"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Contact Email:"
        android:layout_margin="10dp"
        android:layout_gravity="left"
        style="@style/Text1"/>
    </LinearLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="streetValue"
        android:id="@+id/streetValue"
        android:layout_margin="10dp"
        android:layout_gravity="left"
        style="@style/Resp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="cityValue"
        android:id="@+id/cityValue"
        android:layout_margin="10dp"
        android:layout_gravity="left"
        style="@style/Resp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="stateValue"
        android:id="@+id/stateValue"
        android:layout_margin="10dp"
        android:layout_gravity="left"
        style="@style/Resp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="zipcodeValue"
        android:id="@+id/zipcodeValue"
        android:layout_margin="10dp"
        android:layout_gravity="left"
        style="@style/Resp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="proptypeValue"
        android:id="@+id/proptypeValue"
        android:layout_margin="10dp"
        android:layout_gravity="left"
        style="@style/Resp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="numRoomsValue"
        android:id="@+id/numRoomsValue"
        android:layout_margin="10dp"
        android:layout_gravity="left"
        style="@style/Resp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="numBathsValue"
        android:id="@+id/numBathsValue"
        android:layout_margin="10dp"
        android:layout_gravity="left"
        style="@style/Resp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="sqFeetValue"
        android:id="@+id/sqFeetValue"
        android:layout_margin="10dp"
        android:layout_gravity="left"
        style="@style/Resp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="montlyRentValue"
        android:id="@+id/monthlyRentValue"
        android:layout_margin="10dp"
        android:layout_gravity="left"
        style="@style/Resp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="descriptionValue"
        android:id="@+id/descriptionValue"
        android:layout_margin="10dp"
        android:layout_gravity="left"
        style="@style/Resp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="depositValue"
        android:id="@+id/depositValue"
        android:layout_margin="10dp"
        android:layout_gravity="left"
        style="@style/Resp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="leaseTypeValue"
        android:id="@+id/leaseTypeValue"
        android:layout_margin="10dp"
        android:layout_gravity="left"
        style="@style/Resp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="contactNumberValue"
        android:id="@+id/contactNumberValue"
        android:layout_margin="10dp"
        android:layout_gravity="left"
        style="@style/Resp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="contactEmailValue"
        android:id="@+id/contactEmailValue"
        android:layout_margin="10dp"
        android:layout_gravity="left"
        style="@style/Resp"/>


    </LinearLayout>
    </ScrollView>
</RelativeLayout>
