<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="project.team.cmpe277.com.magicrentals1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="project.team.cmpe277.com.magicrentals1.LoginActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="project.team.cmpe277.com.magicrentals1.TenantSearchActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name="project.team.cmpe277.com.magicrentals1.TenantSearchListActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name="project.team.cmpe277.com.magicrentals1.TenantSearchDetailActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value="project.team.cmpe277.com.magicrentals1.TenantSearchListActivity"/>
        </activity>
	<activity
            android:name=".landlord.PropertiesListLandlordActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".landlord.UploadPropertyDataActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme"></activity>
        <activity
            android:name=".landlord.PropertyDetailActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                    android:value="project.team.cmpe277.com.magicrentals1.landlord.PropertiesListLandlordActivity"></meta-data>
        </activity>
        <activity
            android:name=".landlord.EditPropertiesActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme"></activity>
        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />
        <activity
            android:name="com.facebook.FacebookActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:theme="@android:style/Theme.Translucent.NoTitleBar"
            android:label="@string/app_name" />

        <receiver
            android:name="com.google.android.gms.gcm.GcmReceiver"
            android:exported="true"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <category android:name="project.team.cmpe277.com.magicrentals1" />
            </intent-filter>
        </receiver>
        <service
            android:name="project.team.cmpe277.com.magicrentals1.GCMPushReceiverService"
            android:exported="false" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
            </intent-filter>
        </service>
        <service
            android:name="project.team.cmpe277.com.magicrentals1.GCMRegistrationIntentService" android:exported="false">
        <intent-filter>
            <action android:name="com.google.android.gms.iid.InstanceID"/>
        </intent-filter>
    </service>
        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <permission android:name="project.team.cmpe277.com.magicrentals1.permission.C2D_MESSAGE" android:protectionLevel="signature" />
    <uses-permission android:name="project.team.cmpe277.com.magicrentals1.permission.C2D_MESSAGE" />

</manifest>
